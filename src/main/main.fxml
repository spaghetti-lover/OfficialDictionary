<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0" styleClass="background" stylesheets="@../style/navigation.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.MainController">
   <children>

      <AnchorPane fx:id="mainContent" layoutX="104.0" layoutY="158.0" prefHeight="560.0" prefWidth="800.0" />
      <ButtonBar layoutX="185.0" layoutY="36.0" prefHeight="84.0" prefWidth="626.0" style="-fx-background-radius: 32; -fx-background-color: white;">
        <buttons>
            <Button fx:id="searchButton" contentDisplay="TOP" mnemonicParsing="false" onAction="#showSearchPane" prefHeight="58.0" prefWidth="57.0" text="Tra từ" textFill="#000000">
               <font>
                  <Font size="13.0" />
               </font>
               <styleClass>
                  <String fx:value="text-navi-bar" />
                  <String fx:value="button-search" />
               </styleClass>
            </Button>
            <Button fx:id="historyButton" contentDisplay="TOP" mnemonicParsing="false" onAction="#showHistoryPane" prefHeight="58.0" prefWidth="57.0" styleClass="button-history" text="Lịch sử" textFill="#000000">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <Button fx:id="translateButton" contentDisplay="TOP" mnemonicParsing="false" onAction="#showTranslatePane" prefHeight="58.0" prefWidth="57.0" text="Google" textFill="#000000">
               <font>
                  <Font size="13.0" />
               </font>
               <styleClass>
                  <String fx:value="text-navi-bar" />
                  <String fx:value="button-translate" />
               </styleClass>
            </Button>
            <Button fx:id="bookmarkButton" contentDisplay="TOP" mnemonicParsing="false" onAction="#showBookmarkPane" prefHeight="58.0" prefWidth="57.0" text="Đã lưu" textFill="#000000">
               <font>
                  <Font size="13.0" />
               </font>
               <styleClass>
                  <String fx:value="text-navi-bar" />
                  <String fx:value="button-favourite" />
               </styleClass>
            </Button>
            <Button fx:id="gameButton" contentDisplay="TOP" mnemonicParsing="false" onAction="#showGamePane" prefHeight="58.0" prefWidth="222.0" text="Game" textFill="#000000">
               <font>
                  <Font size="13.0" />
               </font>
               <styleClass>
                  <String fx:value="text-navi-bar" />
                  <String fx:value="button-game" />
               </styleClass>
            </Button>
            <Button fx:id="settingButton" contentDisplay="TOP" minWidth="66.0" mnemonicParsing="false" onAction="#showSettingPane" prefHeight="63.0" prefWidth="57.0" text="Cài đặt" textFill="#000000">
               <font>
                  <Font size="13.0" />
               </font>
               <styleClass>
                  <String fx:value="text-navi-bar" />
                  <String fx:value="button-setting" />
               </styleClass>
            </Button>
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
